{
  "manifest_version": 2,
  "name": "Event Durations for Google Calendar",
  "description": "Shows event durations on Google Calendar.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.1.1.min.js",
        "lodash.core.min.js",
        "moment.js",
        "moment-duration-format.js",
        "content.js"
      ],
      "matches": [
        "https://calendar.google.com/*"
      ]
    }
  ],
  "permissions": ["activeTab"]
}
